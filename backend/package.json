{
  "name": "stardeos-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "cp .env.dev .env && nest build",
    "build:staging": "cp .env.staging .env && nest build",
    "build:qa": "cp .env.qa .env && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cp .env.dev .env && nest start --watch",
    "start:staging": "cp .env.staging .env && nest start --watch",
    "start:qa": "cp .env.qa .env && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cp .env.prod .env && node ./dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/nestjs-stripe": "^0.4.1",
    "@golevelup/nestjs-webhooks": "^0.2.14",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "aws-sdk": "^2.860.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.6.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "get-video-dimensions": "^1.0.0",
    "html-pdf": "^3.0.1",
    "image-size": "^0.9.4",
    "lodash": "^4.17.21",
    "moment": "^2.22.2",
    "mongoose": "^5.11.10",
    "multer": "^1.4.2",
    "mustache": "^4.2.0",
    "nestjs-stripe": "^1.0.0",
    "node": "^16.14.0",
